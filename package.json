    "typescript": "^5.1.3",
    "@tsconfig/node18": "2.0.1",
    "hygen": "^6.2.11"
  },
  "workspaces": [
    "packages/*"
  ],
  "overrides": {
    "graphql": "16.5.0"
  }
}